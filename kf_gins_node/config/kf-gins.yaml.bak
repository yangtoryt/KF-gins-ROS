kf_gins_ros2_node:
  ros__parameters:
    # IMU和GNSS数据文件路径
    imupath: "/home/yang/KF-ROS/ros_ws/src/kf_gins_node/config/dataset/Leador-A15.txt"
    gnsspath: "/home/yang/KF-ROS/ros_ws/src/kf_gins_node/config/dataset/GNSS-RTK.txt"

    # 输出文件夹路径
    outputpath: "/home/yang/KF-ROS/ros_ws/src/kf_gins_node/config/dataset"

    # IMU文件列数 (只会用前7列IMU数据)
    imudatalen: 7
    # IMU原始数据频率
    imudatarate: 200

    # 处理时间段
    starttime: 456300
    endtime: -1

    # 初始状态
    initpos: [30.4447873701, 114.4718632047, 20.899]
    initvel: [0.0, 0.0, 0.0]
    initatt: [0.85421502, -2.03480295, 185.70235133]

    # 初始IMU零偏和比例因子
    initgyrbias: [0.0, 0.0, 0.0]
    initaccbias: [0.0, 0.0, 0.0]
    initgyrscale: [0.0, 0.0, 0.0]
    initaccscale: [0.0, 0.0, 0.0]

    # 初始状态标准差
    initposstd: [0.005, 0.004, 0.008]
    initvelstd: [0.003, 0.004, 0.004]
    initattstd: [0.003, 0.003, 0.023]

    # IMU噪声建模参数
    imunoise:
      arw: [0.003, 0.003, 0.003]
      vrw: [0.03, 0.03, 0.03]
      gbstd: [0.027, 0.027, 0.027]
      abstd: [15.0, 15.0, 15.0]
      gsstd: [300.0, 300.0, 300.0]
      asstd: [300.0, 300.0, 300.0]
      corrtime: 4.0

    # 天线杆臂
    antlever: [0.136, -0.301, -0.184]
